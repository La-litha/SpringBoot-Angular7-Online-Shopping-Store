 pipeline {
    agent any
            
        stages {
           stage('SCM') {
      steps {
         git 'https://github.com/La-litha/SpringBoot-Angular7-Online-Shopping-Store.git'
       }
    }
   stage("Build") {
 
     steps {
    bat '''
           cd backend
           mvn clean install
	   java -version
           mvn -version
           mvn clean package
                   
       '''
    }
   }
  }           
}
            
       
